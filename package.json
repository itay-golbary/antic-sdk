{
  "name": "antic-sdk",
  "version": "0.5.1",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "description": "antic task",
  "publishConfig": {
    "access": "restricted"
  },
  "peerDependencies": {
    "react": ">=16.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.2",
    "@types/react": "^18.0.25",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "babel-jest": "^29.3.1",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.3.1",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^29.3.1",
    "jest-junit": "^14.0.1",
    "prettier": "^2.7.1",
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "lint": "eslint --fix \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "lint:ci": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\" -f junit -o ./reports/report-lint.xml",
    "jest-clear": "jest --clearCache",
    "test": "jest --config jestconfig.json",
    "test:ci": "npm run test --ci",
    "clean": "rm -rf dist",
    "build:esm": "tsc --project tsconfig.build.json --module esnext --outDir dist/esm",
    "build:cjs": "tsc --project tsconfig.build.json --module commonjs --outDir dist/cjs",
    "build:ci": "npm run build:esm && npm run build:cjs",
    "build": "npm run clean && npm run build:ci",
    "local-ci": "npm run lint:ci && npm run test && npm run build",
    "prepare": "npm run build:ci"
  },
  "author": "itay@golbary.io",
  "license": "ISC",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itay-golbary/package-test.git"
  },
  "bugs": {
    "url": "https://github.com/itay-golbary/package-test/issues"
  },
  "homepage": "https://github.com/itay-golbary/package-test#readme",
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "styled-components": "^5.3.6"
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
